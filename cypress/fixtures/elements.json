{
    "form": ".account_form",

    "homePage": {
        "buttons": {
            "loginPage": ".fa.fa-user",
            "registerPage": ".fa.fa-lock"
        },

        "imgLogo": ".header-logo"
    },


    "loginPage": {
        "fields": {
            "errorMessage": ".invalid_input",
            "email": "#user",
            "password": "#password",
            "titleSuccessLogin": ".swal2-title",
            "subtitleSuccessLogin": ".swal2-html-container"
        },

        "buttons": {
            "confirmLogin": "#btnLogin"
        },

        "messages": {
            "email": "E-mail inválido.",
            "password": "Senha inválida."
        }
    },


    "registerPage": {
        "fields": {
            "name": "#user",
            "email": "#email",
            "password": "#password",
            "errorMessage": "#errorMessageFirstName",
            "titleSuccessRegister": ".swal2-title",
            "subtitleSuccessRegister": ".swal2-html-container"
        },

        "buttons": {
            "confirmRegister": "#btnRegister"
        },

        "messages": {
            "name": "O campo nome deve ser prenchido",
            "email": "O campo e-mail deve ser prenchido corretamente",
            "password": "O campo senha deve ter pelo menos 6 dígitos"
        }
    }
}